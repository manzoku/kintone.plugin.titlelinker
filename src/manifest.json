{
    "manifest_version": 1,
    "version": 2,
    "type": "APP",
    "name": {
        "ja": "タイトルリンカー",
        "en": "title linker"
    },
    "description": {
        "ja": "レコードのタイトル列（任意の列）に対して詳細表示用リンクを設定します。",
        "en": "Add link to title column for show detail."
    },
    "icon": "image/icon.png",
    "homepage_url": {
        "ja": "https://manzoku.github.io/",
        "en": "https://manzoku.github.io/"
    },
    "desktop": {
        "js": [
            "https://js.cybozu.com/jquery/3.2.1/jquery.min.js",
            "js/titlelinker.js"
        ]
    },
    "config": {
        "html": "html/config.html",
        "js": [
            "https://js.cybozu.com/jquery/3.2.1/jquery.min.js",
            "js/config.js"
        ],
        "css": [
            "css/config.css"
        ]
    }
}
